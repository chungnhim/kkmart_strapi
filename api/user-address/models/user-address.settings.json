{
  "kind": "collectionType",
  "collectionName": "user_addresses",
  "info": {
    "name": "UserAddress"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "user_addresses"
    },
    "country": {
      "model": "country",
      "via": "user_addresses"
    },
    "is_default": {
      "type": "boolean"
    },
    "state": {
      "model": "state",
      "via": "user_addresses"
    },
    "address1": {
      "type": "string"
    },
    "address2": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "postcode": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "is_default_billing": {
      "type": "boolean"
    }
  }
}
