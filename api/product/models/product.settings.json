{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "supplierid": {
      "type": "integer"
    },
    "productkeyword": {
      "type": "string"
    },
    "categoryid": {
      "type": "integer"
    },
    "shortdescription": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "float"
    },
    "currencycode": {
      "type": "integer"
    },
    "currencycodeiso": {
      "type": "string"
    },
    "retailprice": {
      "type": "float"
    },
    "status": {
      "type": "integer"
    },
    "sku": {
      "type": "string"
    },
    "brand": {
      "model": "brand"
    },
    "stock": {
      "type": "integer"
    },
    "soldqtty": {
      "type": "integer"
    },
    "minimumorderqtty": {
      "type": "integer"
    },
    "maximumorderqtty": {
      "type": "integer"
    },
    "weight": {
      "type": "integer"
    },
    "rating_point": {
      "type": "float"
    },
    "barcode": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "product_variants": {
      "via": "product_id",
      "collection": "product-variant"
    },
    "product_ratings": {
      "collection": "product-rating",
      "via": "product"
    },
    "shopping_cart_products": {
      "via": "product",
      "collection": "shopping-cart-product"
    },
    "promotionproduct": {
      "model": "promotionproduct",
      "via": "products"
    },
    "flashsaleproduct": {
      "via": "products",
      "model": "flashsaleproducts"
    },
    "productimgs": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "categories": {
      "collection": "category",
      "via": "products",
      "dominant": true
    },
    "order_products": {
      "via": "product",
      "collection": "order-product"
    },
    "coin_earn": {
      "type": "float"
    },
    "coin_use": {
      "type": "float"
    },
    "can_use_coin": {
      "type": "boolean"
    }
  }
}
